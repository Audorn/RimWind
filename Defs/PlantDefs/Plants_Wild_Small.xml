<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<ThingDef ParentName="PlantBase">
		<defName>PlantWildStoneflower</defName> 
		<label>stoneflower</label>
		<description>Stoneflower petals are collected from a small common blue flower of the Ascadian Isles and Azura's Coast.</description>

		<graphicData>
			<texPath>Plants/Small/Stoneflower</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<pathCost>10</pathCost>

		<selectable>true</selectable>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
		</statBases>
		
		<plant>
			<fertilitySensitivity>0.30</fertilitySensitivity>
			<topWindExposure>0.3</topWindExposure>

			<wildClusterRadius>3</wildClusterRadius>
			<wildClusterSizeRange>
				<min>3</min>
				<max>7</max>
			</wildClusterSizeRange>
			
			<visualSizeRange>
				<min>0.3</min>
				<max>0.7</max>
			</visualSizeRange>
			<immatureGraphicPath>Plants/Small/Stoneflower_Immature</immatureGraphicPath>
			<leaflessGraphicPath>Plants/Small/Stoneflower_Leafless</leaflessGraphicPath>
			<maxMeshCount>4</maxMeshCount>

			<growDays>3</growDays>
			<harvestTag>Standard</harvestTag>
			<harvestFailable>true</harvestFailable>
			<harvestMinGrowth>0.85</harvestMinGrowth>
			<harvestAfterGrowth>0.30</harvestAfterGrowth>
			<harvestWork>350</harvestWork>
			<harvestedThingDef>StoneflowerPetals</harvestedThingDef>
			<harvestYield>2</harvestYield>
			<dieIfLeafless>false</dieIfLeafless>
			<reproduceMtbDays>3</reproduceMtbDays>
			<reproduceRadius>5</reproduceRadius>
		</plant>
		
		<comps>
			<li Class="RimTES.CompProperties_HarvestableReagent">
			</li>
		</comps>
		
		<ingestible>
			<nutrition>0.15</nutrition>
		</ingestible>		
	</ThingDef>

