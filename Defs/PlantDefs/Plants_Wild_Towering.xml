			<texPath>Plants/Towering/OgrimShield</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>

		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<Flammability>0.65</Flammability>
		</statBases>
		
