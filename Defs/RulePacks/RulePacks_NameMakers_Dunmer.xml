<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<RulePackDef>
		<defName>NamerPersonDunmer</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[dunmermalename] [dunmerlastname]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>dunmerlastname</keyword>
					<path>DunmerNames/lastname</path>
				</li>
				<li Class="Rule_File">
					<keyword>dunmermalename</keyword>
					<path>DunmerNames/malename</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>NamerPersonDunmerFemale</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[dunmerfemalename] [dunmerlastname]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>dunmerlastname</keyword>
					<path>DunmerNames/lastname</path>
				</li>
				<li Class="Rule_File">
					<keyword>dunmerfemalename</keyword>
					<path>DunmerNames/femalename</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
	
</Defs>